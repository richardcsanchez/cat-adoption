<%= form_for @agency do |f|%>

<%=f.label 'name'%>: <br>
<%=f.text_field :name %> <br>
<br>
<%=f.label 'street_1'%>: <br>
<%=f.text_field :street_1 %> <br>
<br>
<%=f.label 'street_2'%>: <br>
<%=f.text_field :street_2 %> <br>
<br>
<%=f.label 'state'%>: <br>
<%=f.text_field :state %> <br>
<br>
<%= f.label 'zip_code' %>:<br>
<%= f.text_field :zip_code%><br>
<br>
<%= f.label 'email' %>:<br>
<%= f.text_field :email%><br>
<br>
<%=f.label 'phone_number'%>:  <br>
<%=f.text_field :phone_number %> <br>
<br>
<%= f.submit "Submit" %>
<%end%>

<br>
